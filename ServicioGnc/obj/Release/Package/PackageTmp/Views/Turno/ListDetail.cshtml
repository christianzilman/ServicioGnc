@{
    ViewBag.Title = "ListDetail";
    Layout = null;
}

@{
    List<ServicioGnc.Models.DetalleTurno> listDetalleTurno = ViewBag.DetalleTurnos;
}
<table class="table">
    <tr class="warning">
        <th>
            Horario
        </th>
        <th>
            Dia
        </th>
        <th></th>
    </tr>

    @foreach (var item in listDetalleTurno)
    {
        <tr>
            <td>
                @item.Horario.ComienzoTurno  --- @item.Horario.FinalTurno 
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dia)
            </td>
            <td>
                <a href="javascript:eliminarDetalle(@item.HorarioId)">Eliminar</a>
            </td>
        </tr>
    }

</table>

