@{
    ViewBag.Title = "AgregarDetalle";
    Layout = null;
}

@{
    List<ServicioGnc.Models.Carro> listCarro = ViewBag.ListCarro;
     
    <br/>
    <div class="alert alert-warning" role="alert">
        <span class="glyphicon glyphicon-th-list" aria-hidden="true"></span>
    Listado de Pedido
</div>

 
        
    <table class="table">
        <tr class="warning">
            <td>Concepto</td>
            <td>Importe</td>
            <td></td>
        </tr>
        @foreach(ServicioGnc.Models.Carro carro in listCarro){
            <tr>
                
                <td>@carro.Producto.Nombre</td>
                
                <td>@carro.Precio</td>
                <td><input type="button" value="Quitar" class="btn-warning" onclick="javascript:EliminarDetalle()" /></td>
            </tr>   
        }
    </table>
    
    
}