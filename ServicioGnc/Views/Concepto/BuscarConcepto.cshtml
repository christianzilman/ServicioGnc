@model IEnumerable<ServicioGnc.Models.Concepto>
@{
    ViewBag.Title = "BuscarConcepto";
    Layout = null;
}

<br/>

<div class="alert alert-warning" role="alert">
    <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
    Resultados de búsqueda
</div>

<table class="table">
    <thead>
        <tr class="warning">
            <th hidden="hidden">
                Código
            </th>

            <th>
               Producto
            </th>

            <th>
                Precio
            </th>

        </tr>
    </thead>
    <tbody>

            <tr>
                <td hidden="hidden">
                    <input type="text" value="@ViewBag.ConceptoId" id="ConceptoIdPanel" name="ConceptoIdPanel" />
                </td>

                <td>
                    <label>
                        @ViewBag.Nombre
                    </label>
                </td>
                <td>
                    <label>@ViewBag.Importe</label>
                </td>

            </tr>
        
    <tbody>
</table>

<input type="button" value="Agregar" class="btn-warning" onclick="javascript:AgregarDetalleLiquidacion()"/>
<br/>