<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Servicio Gnc</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bootstrap-3.1.1/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-3.1.1/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-3.1.1/css/bootstrap-theme.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Content/bootstrap-3.1.1/js/bootstrap.js"></script>
    <script src="~/Content/bootstrap-3.1.1/js/moment.js"></script>
    <script src="~/Content/bootstrap-3.1.1/js/bootstrap-datetimepicker.js"></script>

    <!--<link href="~/Content/bootstrap-3.1.1/css/sticky-footer-navbar.css" rel="stylesheet" />-->
    <!--<link href="~/Content/bootstrap-3.1.1/css/navbar-fixed-top.css" rel="stylesheet" />-->
</head>
<body role="document">
    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Servicio Gnc</a>
            </div>
            <div class="navbar-collapse collapse navbar-right">
                <section id="login">
                    @Html.Partial("_LoginPartial")
                </section>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </div>
    <div id="body" class="container theme-showcase" role="main">
        <br /><br /><br />
        @RenderSection("featured", required: false)
        <!--<section class="content-wrapper main-content clear-fix">-->
        @RenderBody()

    </div> <!-- /container -->
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    @RenderSection("scripts", required: false)
</body>

</html>