@model ServicioGnc.Models.TipoConcepto

@{
    ViewBag.Title = "Tipo de Concepto";
}

<h2>Eliminar Tipo de Concepto</h2>

<div class="panel panel-warning">
    <div class="panel-heading">
        <h2 class="panel-title">Tipo de Concepto - ¿Está seguro de que desea eliminar esto?</h2>
    </div>
    <div class="panel-body">
        <div class="col-md-3">
            <div>    Nombre: @Html.DisplayFor(model => model.Nombre) </div>
            <br />
            <div>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <p>
                        <input type="submit" class="btn-warning" value="Eliminar" /> |
                        @Html.ActionLink("Volver al listado", "Index")
                    </p>
                }

            </div>
        </div>
    </div>
</div>
